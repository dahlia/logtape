{
  "name": "@logtape/adaptor-pino",
  "version": "1.2.8",
  "license": "MIT",
  "exports": "./src/mod.ts",
  "imports": {
    "pino-abstract-transport": "npm:pino-abstract-transport@^2.0.0"
  },
  "exclude": [
    "coverage/",
    "npm/",
    ".dnt-import-map.json"
  ],
  "tasks": {
    "build": "pnpm build",
    "test": "deno test --allow-env --allow-sys",
    "test:node": {
      "dependencies": [
        "build"
      ],
      "command": "node --experimental-transform-types --test"
    },
    "test:bun": {
      "dependencies": [
        "build"
      ],
      "command": "bun test"
    },
    "test-all": {
      "dependencies": [
        "test",
        "test:node",
        "test:bun"
      ]
    }
  }
}
