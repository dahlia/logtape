{
  "name": "@logtape/file",
  "version": "1.2.0",
  "license": "MIT",
  "exports": "./src/mod.ts",
  "imports": {
    "#filesink": "./src/filesink.jsr.ts"
  },
  "exclude": [
    "coverage/",
    "npm/",
    ".dnt-import-map.json"
  ],
  "tasks": {
    "build": "pnpm build",
    "test": "deno test --allow-read --allow-write --allow-env=TEMP,TMP,TMPDIR",
    "test:node": {
      "dependencies": [
        "build"
      ],
      "command": "node --experimental-transform-types --test"
    },
    "test:bun": {
      "dependencies": [
        "build"
      ],
      "command": "bun test"
    },
    "test-all": {
      "dependencies": [
        "test",
        "test:node",
        "test:bun"
      ]
    }
  }
}
