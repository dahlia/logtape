{
  "name": "@logtape/express",
  "version": "2.0.1",
  "license": "MIT",
  "exports": "./src/mod.ts",
  "exclude": [
    "coverage/",
    "npm/",
    "dist/"
  ],
  "tasks": {
    "build": "pnpm build",
    "test": "deno test --allow-env --allow-sys --allow-net",
    "test:node": {
      "dependencies": [
        "build"
      ],
      "command": "node --experimental-transform-types --test"
    },
    "test:bun": {
      "dependencies": [
        "build"
      ],
      "command": "bun test"
    },
    "test-all": {
      "dependencies": [
        "test",
        "test:node",
        "test:bun"
      ]
    }
  }
}
